#! /bin/sh
set -x

libtoolize --force --copy
#aclocal-1.9
aclocal
#autoheader2.50
autoheader
#add --include-deps if you want to bootstrap with any other compiler than gcc
#automake --add-missing --copy --include-deps
automake-1.9 --add-missing --force --copy
#autoconf2.50
autoconf
rm -f config.cache
