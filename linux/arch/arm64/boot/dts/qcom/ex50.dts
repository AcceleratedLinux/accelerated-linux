/dts-v1/;

#include "ipq6018.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	model = "Digi EX50";
	compatible = "digi,ex50", "qcom,ipq6018";

	reserved-memory {
		/*
		 * 64KiB reserved for pstore (ramoops). U-Boot reserves 4k at
		 * the very end of the 512MB memory. Otherwise everything else
		 * U-Boot is using is before 256MB.
		 * Kernel is using the end of the memory. But a bunch of memory
		 * is reserved for the CPU around 0x50000000, like
		 * "smem_region", "q6_etr_region", etc. The last one is the
		 * "q6_etr_region", which is finished at 0x500fffff. Reserve
		 * region for ramoops just after that
		 */
		ramoops@50100000 {
			compatible = "ramoops";
			reg = <0x0 0x50100000 0x0 0x10000>;
			ecc-size = <16>;
			record-size = <0x1000>;
			console-size = <0x1000>;
		};
	};

	aliases {
		serial0 = &blsp1_uart3;
	};

	chosen {
		bootargs-append = " swiotlb=1";
	};

	i2c-gpio-0 {
		compatible = "i2c-gpio";
		sda-gpios = <&tlmm 25 (GPIO_ACTIVE_HIGH | GPIO_OPEN_DRAIN)>;
		scl-gpios = <&tlmm 22 (GPIO_ACTIVE_HIGH | GPIO_OPEN_DRAIN)>;
		status = "okay";

		#address-cells = <1>;
		#size-cells = <0>;

		extgpio: pca8575@20 {
			compatible = "nxp,pca8575";
			reg = <0x20>;
			gpio-controller;
			#gpio-cells = <2>;
			interrupt-parent = <&tlmm>;
			interrupts = <27 IRQ_TYPE_EDGE_FALLING>;
			pinctrl-names = "default";
			pinctrl-0 = <&i2c_gpio_led_pins>;
		};
	};

	i2c-gpio-1 {
		compatible = "i2c-gpio";
		sda-gpios = <&tlmm 24 (GPIO_ACTIVE_HIGH | GPIO_OPEN_DRAIN)>;
		scl-gpios = <&tlmm 23 (GPIO_ACTIVE_HIGH | GPIO_OPEN_DRAIN)>;
		pinctrl-names = "default";
		pinctrl-0 = <&i2c_gpio_atmel_pins>;
		status = "okay";

		#address-cells = <1>;
		#size-cells = <0>;

		rng: atecc508@60 {
			compatible = "atsha204-i2c";
			reg = <0x60>;
		};
	};

	nss-macsec0 {
		compatible = "qcom,nss-macsec";
		phy_addr = <16>;
		phy_access_mode = <0>;
		mdiobus = <&mdio>;
	};

	nss-macsec1 {
		compatible = "qcom,nss-macsec";
		phy_addr = <24>;
		phy_access_mode = <0>;
		mdiobus = <&mdio>;
	};

	ess-switch@3a000000 {
		switch_cpu_bmp = <0x1>;  /* cpu port bitmap */
		switch_lan_bmp = <0x10>; /* lan port bitmap */
		switch_wan_bmp = <0x20>; /* wan port bitmap */
		switch_inner_bmp = <0xc0>; /*inner port bitmap*/
		switch_mac_mode = <0xf>; /* mac mode for uniphy instance0*/
		switch_mac_mode1 = <0xf>; /* mac mode for uniphy instance1*/
		switch_mac_mode2 = <0xff>; /* mac mode for uniphy instance2*/

		qcom,port_phyinfo {
			port@4 {
				port_id = <4>;
				phy_address = <16>;
				port_mac_sel = "QGMAC_PORT";
			};
			port@5 {
				port_id = <5>;
				phy_address = <24>;
				port_mac_sel = "QGMAC_PORT";
			};
		};
	};

	dp1 {
		device_type = "network";
		compatible = "qcom,nss-dp";
		qcom,id = <4>;
		reg = <0x0 0x3a001600 0x0 0x200>;
		qcom,mactype = <0>;
		local-mac-address = [000000000000];
		qcom,link-poll = <1>;
		qcom,phy-mdio-addr = <16>;
		phy-mode = "sgmii";
	};

	dp2 {
		device_type = "network";
		compatible = "qcom,nss-dp";
		qcom,id = <5>;
		reg = <0x0 0x3a001800 0x0 0x200>;
		qcom,mactype = <0>;
		local-mac-address = [000000000000];
		qcom,link-poll = <1>;
		qcom,phy-mdio-addr = <24>;
		phy-mode = "sgmii";
	};

	power-monitor {
		compatible = "digi,ex50-power";

		poe-present-gpios = <&tlmm 18 GPIO_ACTIVE_LOW>;
		poe-mec-gpios     = <&tlmm 71 GPIO_ACTIVE_LOW>;
		dc-present-gpios  = <&tlmm 34 GPIO_ACTIVE_LOW>;
	};

	sim-mux0 {
		compatible = "sim-mux";

		sim-select-gpios  = <&tlmm 78 GPIO_ACTIVE_LOW>;
		sim-present-gpios = <&tlmm  0 GPIO_ACTIVE_HIGH>,
				    <&tlmm 62 GPIO_ACTIVE_HIGH>;

		sim-count = <2>;
	};
};

&soc {
	gpio-keys {
		compatible = "gpio-keys";

		factory-button {
			label = "factory";
			linux,code = <KEY_DELETE>;
			gpios = <&tlmm 68 GPIO_ACTIVE_LOW>;
		};

		sim-button {
			label = "sim_switch";
			linux,code = <KEY_SELECT>;
			gpios = <&tlmm 79 GPIO_ACTIVE_LOW>;
		};
	};
};

&soc {
	leds {
		compatible = "gpio-leds";
		pinctrl-0 = <&leds_pins>;
		pinctrl-names = "default";

		led@29 {
			label = "power-green";
			gpios = <&tlmm 29 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "none";
			default-state = "keep";
		};

		led@30 {
			label = "power-blue";
			gpios = <&tlmm 30 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "none";
			default-state = "keep";
		};

		led@72 {
			label = "power-red";
			gpios = <&tlmm 72 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "none";
			default-state = "keep";
		};

		led@37 {
			label = "wifi-5g";
			gpios = <&tlmm 37 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "none";
			default-state = "off";
		};

		led@35 {
			label = "wifi-2.4g";
			gpios = <&tlmm 35 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "none";
			default-state = "off";
		};

		extgpio@3 {
			label = "SIM1";
			gpios = <&extgpio 3 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "none";
			default-state = "off";
		};

		extgpio@4 {
			label = "SIM2";
			gpios = <&extgpio 4 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "none";
			default-state = "off";
		};

		extgpio@5 {
			label = "RSS5";
			gpios = <&extgpio 5 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "none";
			default-state = "off";
		};

		extgpio@6 {
			label = "RSS4";
			gpios = <&extgpio 6 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "none";
			default-state = "off";
		};

		extgpio@7 {
			label = "RSS3";
			gpios = <&extgpio 7 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "none";
			default-state = "off";
		};

		extgpio@8 {
			label = "RSS2";
			gpios = <&extgpio 8 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "none";
			default-state = "off";
		};

		extgpio@9 {
			label = "RSS1";
			gpios = <&extgpio 9 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "none";
			default-state = "off";
		};

		extgpio@10 {
			label = "cell-blue";
			gpios = <&extgpio 10 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "none";
			default-state = "off";
		};

		extgpio@11 {
			label = "cell-green";
			gpios = <&extgpio 11 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "none";
			default-state = "off";
		};

		extgpio@12 {
			label = "cell-red";
			gpios = <&extgpio 12 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "none";
			default-state = "off";
		};

		extgpio@13 {
			label = "online-blue";
			gpios = <&extgpio 13 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "none";
			default-state = "off";
		};

		extgpio@14 {
			label = "online-green";
			gpios = <&extgpio 14 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "none";
			default-state = "off";
		};

		extgpio@15 {
			label = "online-red";
			gpios = <&extgpio 15 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "none";
			default-state = "off";
		};
	};
};

&blsp1_uart3 {
	pinctrl-0 = <&serial_3_pins>;
	pinctrl-names = "default", "serial-gpio-pins";
	dtr-gpios = <&tlmm 57 GPIO_ACTIVE_LOW>;
	dcd-gpios = <&tlmm 16 GPIO_ACTIVE_LOW>;
	rts-gpios = <&tlmm 69 GPIO_ACTIVE_LOW>;
	cts-gpios = <&tlmm 70 GPIO_ACTIVE_LOW>;
	status = "okay";
};

&blsp1_i2c3 {
	pinctrl-0 = <&i2c_1_pins>;
	pinctrl-names = "default";
	status = "okay";

	accelerometer@1e {
		compatible = "st,lis2hh12";
		reg = <0x1e>;
	};
};

&qpic_bam {
	status = "okay";
};

&qpic_nand {
	status = "okay";

	nand@0 {
		reg = <0>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "boot";
				reg = <0x00000000 0x02000000>;
			};
			partition@1 {
				label = "flash";
				reg = <0x02000000 0>;
			};
		};
	};
};

&blsp1_spi1 {
	cs-select = <0>;
	status = "okay";

	nor@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		reg = <0>;
		compatible = "jedec,spi-nor";
		spi-max-frequency = <50000000>;

		partition@0 {
			label = "0:SBL1";
			reg = <0x00 0xc0000>;
		};

		partition@c0000 {
			label = "0:MIBIB";
			reg = <0xc0000 0x10000>;
		};

		partition@d0000 {
			label = "0:BOOTCONFIG";
			reg = <0xd0000 0x20000>;
		};

		partition@f0000 {
			label = "0:BOOTCONFIG1";
			reg = <0xf0000 0x20000>;
		};

		partition@110000 {
			label = "0:QSEE";
			reg = <0x110000 0x1a0000>;
		};

		partition@2b0000 {
			label = "0:QSEE_1";
			reg = <0x2b0000 0x1a0000>;
		};

		partition@450000 {
			label = "0:DEVCFG";
			reg = <0x450000 0x10000>;
		};

		partition@460000 {
			label = "0:DEVCFG_1";
			reg = <0x460000 0x10000>;
		};

		partition@470000 {
			label = "0:RPM";
			reg = <0x470000 0x40000>;
		};

		partition@4b0000 {
			label = "0:RPM_1";
			reg = <0x4b0000 0x40000>;
		};

		partition@4f0000 {
			label = "0:CDT";
			reg = <0x4f0000 0x10000>;
		};

		partition@500000 {
			label = "0:CDT_1";
			reg = <0x500000 0x10000>;
		};

		partition@510000 {
			label = "0:APPSBLENV";
			reg = <0x510000 0x10000>;
		};

		partition@520000 {
			label = "0:APPSBL";
			reg = <0x520000 0xa0000>;
		};

		partition@5c0000 {
			label = "0:APPSBL_1";
			reg = <0x5c0000 0xa0000>;
		};

		partition@660000 {
			label = "0:ART";
			reg = <0x660000 0x40000>;
			read-only;
		};
	};
};

&mdio {
	pinctrl-0 = <&mdio_pins>;
	pinctrl-names = "default";
	phy-reset-gpio = <&tlmm 75 0 &tlmm 77 1>;
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";

	phy0: ethernet-phy@0 {
		reg = <16>;
	};
	phy1: ethernet-phy@1 {
		reg = <24>;
	};
};

&qusb_phy_0 {
	status = "okay";
};

&qusb_phy_1 {
	status = "okay";
};

&ssphy_0 {
	status = "okay";
};

&usb1 {
	status = "okay";
};

&usb2 {
	status = "okay";
};

&pcie0 {
	status = "disabled";
};

&pcie_phy {
	status = "disabled";
};

&tlmm {
	gpio-line-names = "", "", "", "", "", "", "", "",
	/*  8 */	  "", "", "", "", "", "", "", "",
	/* 16 */	  "dcd", "", "poe_in", "", "", "", "", "",
	/* 24 */	  "", "", "", "", "", "", "", "wdi",
	/* 32 */	  "", "", "dc_in", "", "", "", "", "",
	/* 40 */	  "", "", "", "", "", "", "", "",
	/* 48 */	  "", "", "", "", "", "", "", "",
	/* 56 */	  "", "dtr", "", "", "", "", "", "developer_",
	/* 64 */	  "", "", "", "", "factory", "rts", "cts", "full_power",
	/* 72 */	  "", "", "wdi_enable", "", "", "", "sim_select", "sim_switch";

	i2c_1_pins: i2c-1-pins {
		pins = "gpio42", "gpio43";
		function = "blsp2_i2c";
		drive-strength = <8>;
	};

	spi_0_pins: spi-0-pins {
		pins = "gpio38", "gpio39", "gpio40", "gpio41";
		function = "blsp0_spi";
		drive-strength = <8>;
		bias-pull-down;
	};

	leds_pins: leds-pins {
		pins = "gpio29", "gpio30", "gpio35", "gpio37", "gpio72";
		functions = "gpio";
		drive-strength = <8>;
		bias-pull-down;
	};

	i2c_gpio_led_pins: i2c-gpio-led-pins {
		pins = "gpio22", "gpio25", "gpio27";
		functions = "gpio";
		drive-strength = <8>;
	};

	i2c_gpio_atmel_pins: i2c-gpio-atmel-pins {
		pins = "gpio23", "gpio24";
		functions = "gpio";
		drive-strength = <8>;
	};

	serial_gpio_pins: serial-gpio-pins {
		pins = "gpio16", "gpio57", "gpio69", "gpio70";
		functions = "gpio";
		drive-strength = <8>;
	};

	wdi_gpio_pins: wdi-gpio-pins {
		pins = "gpio31", "gpio74";
		functions = "gpio";
		drive-strength = <8>;
	};

	mdio_pins: mdio_pinmux {
		mux_0 {
			pins = "gpio64";
			function = "mdc";
			drive-strength = <8>;
			bias-pull-up;
		};
		mux_1 {
			pins = "gpio65";
			function = "mdio";
			drive-strength = <8>;
			bias-pull-up;
		};
		mux_2 {
			pins = "gpio75";
			function = "gpio";
			bias-pull-up;
		};
		mux_3 {
			pins = "gpio77";
			function = "gpio";
			bias-pull-up;
		};
	};
};

&wifi0 {
	/* 512MB DDR */
	qcom,tgt-mem-mode = <1>;
};
