/dts-v1/;
#include "armada-388.dtsi"
#include <dt-bindings/gpio/gpio.h>

/ {
	model = "Accelerated Concepts U115";
	compatible = "accelerated,u115", "marvell,armada388",
		"marvell,armada385", "marvell,armada380";

	chosen {
		stdout-path = "serial0:115200n8";
	};

	memory {
		device_type = "memory";
		reg = <0x00000000 0x40000000>; /* 1GB */
	};

	soc {
		ranges = <MBUS_ID(0xf0, 0x01) 0 0xf1000000 0x100000
			  MBUS_ID(0x01, 0x1d) 0 0xfff00000 0x100000
			  MBUS_ID(0x09, 0x19) 0 0xf1100000 0x10000
			  MBUS_ID(0x09, 0x15) 0 0xf1110000 0x10000>;

		internal-regs {

			serial@12000 {
				status = "okay";
			};

			serial@12100 {
				status = "okay";
				pinctrl-0 = <&uart1_full_pins &uart1_dtr_pin &uart1_dcd_pin>;
				pinctrl-names = "default";
				dtr-gpios = <&gpio1 27 GPIO_ACTIVE_LOW>;
				dcd-gpios = <&gpio0 6 GPIO_ACTIVE_LOW>;
			};

			i2c@11000 {
				status = "okay";
				pinctrl-0 = <&i2c0_pins>;
				pinctrl-names = "default";
				clock-frequency = <200000>;

				extgpio: pca8575@26 {
					compatible = "nxp,pca8575";
					reg = <0x26>;
					gpio-controller;
					#gpio-cells = <2>;
				};
			};

			eth0: ethernet@70000 {
				status = "okay";
				phy-mode = "sgmii";
				fixed-link {
					speed = <1000>;
					full-duplex;
				};
			};

			eth1: ethernet@30000 {
				status = "okay";
				phy = <&phy1>;
				phy-mode = "sgmii";
			};

			eth2: ethernet@34000 {
				status = "okay";
				phy = <&phy2>;
				phy-mode = "sgmii";
			};

			usb@58000 {
				status = "okay";
			};

			usb3@f0000 {
				status = "okay";
			};

			usb3@f8000 {
				status = "okay";
			};

			gpio_leds {
				compatible = "gpio-leds";
				pinctrl-names = "default";
				pinctrl-0 = <&online_led_pin>;

				green_online_led {
					label = "u115:green:online";
					gpios = <&gpio1 5 1>;
					linux,default-trigger = "online";
				};

				green_vpn_led {
					label = "u115:green:vpn";
					gpios = <&gpio1 7 1>;
					linux,default-trigger = "vpn";
				};

				green_usb_led {
					label = "u115:green:usb";
					gpios = <&gpio1 11 1>;
					linux,default-trigger = "usb";
				};

				green_wan1_led {
					label = "u115:green:wan1";
					gpios = <&gpio1 12 1>;
					linux,default-trigger = "wan1";
				};

				green_wan2_led {
					label = "u115:green:wan2";
					gpios = <&gpio1 20 1>;
					linux,default-trigger = "wan2";
				};

				green_serial_led {
					label = "u115:green:serial";
					gpios = <&gpio1 21 1>;
					linux,default-trigger = "serial";
				};
			};
		};
	};
};

&nand_controller {
	status = "okay";

	nand@0 {
		reg = <0>;
		label = "pxa3xx_nand-0";
		nand-rb = <0>;
		nand-on-flash-bbt;
		nand-ecc-strength = <4>;
		nand-ecc-step-size = <512>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x00000000 0x00200000>;
			};
			partition@1 {
				label = "u-boot-env";
				reg = <0x00200000 0x00100000>;
			};
			partition@2 {
				label = "log";
				reg = <0x00300000 0x00200000>;
			};
			partition@3 {
				label = "flash";
				reg = <0x00500000 0>;
			};
			partition@4 {
				label = "all";
				reg = <0x00000000 0>;
			};
		};
	};
};

&pinctrl {
	online_led_pin: online-led-pin {
		marvell,pins = "mpp37";
		marvell,function = "gpio";
	};

	vpn_led_pin: vpn-led-pin {
		marvell,pins = "mpp39";
		marvell,function = "gpio";
	};

	usb3_led_pin: usb-led-pin {
		marvell,pins = "mpp43";
		marvell,function = "gpio";
	};

	wan1_led_pin: wan1-led-pin {
		marvell,pins = "mpp44";
		marvell,function = "gpio";
	};

	wan2_led_pin: wan2-led-pin {
		marvell,pins = "mpp52";
		marvell,function = "gpio";
	};

	serial_led_pin: serial-led-pin {
		marvell,pins = "mpp53";
		marvell,function = "gpio";
	};

	uart1_full_pins: uart1-full-pins {
		marvell,pins = "mpp55", "mpp56", "mpp57", "mpp58";
		marvell,function = "ua1";
	};

	uart1_dtr_pin: uart1-dtr-pin {
		marvell,pins = "mpp59";
		marvell,function = "gpio";
	};

	uart1_dcd_pin: uart1-dcd-pin {
		marvell,pins = "mpp6";
		marvell,function = "gpio";
	};

};

&mdio {
	status = "okay";
	pinctrl-0 = <&mdio_pins>;
	pinctrl-names = "default";

	phy1: ethernet-phy@0 {
		status = "okay";
		reg = <0>;
	};

	phy2: ethernet-phy@1 {
		status = "okay";
		reg = <1>;
	};

	switch@10 {
		compatible = "marvell,mv88e6085";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0x10>;
		dsa,member = <0 0>;

		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@1 {
				reg = <1>;
				label = "lan1";
			};
			port@2 {
				reg = <2>;
				label = "lan2";
			};
			port@3 {
				reg = <3>;
				label = "lan3";
			};
			port@4 {
				reg = <4>;
				label = "lan4";
			};
			port@5 {
				reg = <5>;
				label = "lan5";
			};
			port@6 {
				reg = <6>;
				label = "lan6";
			};
			port@7 {
				reg = <7>;
				label = "lan7";
			};
			port@8 {
				reg = <8>;
				label = "lan8";
			};
			port@9 {
				reg = <9>;
				label = "cpu";
				ethernet = <&eth0>;
				phy-mode = "sgmii";
				fixed-link {
					speed = <1000>;
					full-duplex;
				};
			};
		};
	};
};
