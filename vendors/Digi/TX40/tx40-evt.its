/dts-v1/;

/ {
	description = "U-Boot fitImage for Digi TX40 EVT";
	#address-cells = <1>;

	images {
		kernel-1 {
			description = "Linux kernel";
			data = /incbin/("kernel.gz");
			type = "kernel";
			arch = "arm64";
			os = "linux";
			compression = "gzip";
			load = <0x84080000>;
			entry = <0x84080000>;
			hash-1 {
				algo = "sha1";
			};
		};

		fdt-fsl-ls1028a-tx40-evt.dtb {
			description = "Flattened Device Tree blob";
			data = /incbin/("fsl-ls1028a-tx40-evt.dtb");
			type = "flat_dt";
			arch = "arm64";
			compression = "none";
			load = <0x88000000>;
			hash-1 {
				algo = "sha1";
			};
		};
	};

	configurations {
		default = "conf-fsl-ls1028a-tx40-evt.dtb";
		conf-fsl-ls1028a-tx40-evt.dtb {
			description = "1 Linux kernel, FDT blob";
			kernel = "kernel-1";
			fdt = "fdt-fsl-ls1028a-tx40-evt.dtb";
			hash-1 {
				algo = "sha1";
			};
		};
	};
};
