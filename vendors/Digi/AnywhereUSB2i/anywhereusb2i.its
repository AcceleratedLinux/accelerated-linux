/dts-v1/;

/ {
	description = "U-Boot fitImage for Digi AnywhereUSB Plus";
	#address-cells = <1>;

	images {
		kernel@1 {
			description = "Linux kernel";
			data = /incbin/("kernel.gz");
			type = "kernel";
			arch = "arm64";
			os = "linux";
			compression = "gzip";
			load = <0x80000000>;
			entry = <0x80000000>;
			hash@1 {
				algo = "sha1";
			};
		};

		fdt@anywhereusb2-industrial.dtb {
			description = "Flattened Device Tree blob";
			data = /incbin/("anywhereusb2-industrial.dtb");
			/*data = /incbin/("fsl-ls1012a-frwy.dtb");*/
			type = "flat_dt";
			arch = "arm64";
			compression = "none";
			load = <0x90000000>;
			hash@1 {
				algo = "sha1";
			};
		};
	};

	configurations {
		default = "conf@anywhereusb2-industrial.dtb";
		conf@anywhereusb2-industrial.dtb {
			description = "1 Linux kernel, FDT blob";
			kernel = "kernel@1";
			fdt = "fdt@anywhereusb2-industrial.dtb";
			/*fdt = "fdt@fsl-ls1012a-frwy.dtb";*/
			hash@1 {
				algo = "sha1";
			};
		};
	};
};
