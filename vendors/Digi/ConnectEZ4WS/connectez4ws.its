/dts-v1/;

/ {
	description = "U-Boot fitImage for Digi ConnectEZ-4 WS";
	#address-cells = <1>;

	images {
		kernel-1 {
			description = "Linux kernel";
			data = /incbin/("kernel.gz");
			type = "kernel";
			arch = "arm64";
			os = "linux";
			compression = "gzip";
			load = <0x80000000>;
			entry = <0x80000000>;
			hash-1 {
				algo = "sha1";
			};
		};

		fdt-connectez4ws.dtb {
			description = "Flattened Device Tree blob";
			data = /incbin/("connectez4ws.dtb");
			type = "flat_dt";
			arch = "arm64";
			compression = "none";
			load = <0x84000000>;
			hash-1 {
				algo = "sha1";
			};
		};
	};

	configurations {
		default = "conf-connectez4ws.dtb";
		conf-connectez4ws.dtb {
			description = "1 Linux kernel, FDT blob";
			kernel = "kernel-1";
			fdt = "fdt-connectez4ws.dtb";
			hash-1 {
				algo = "sha1";
			};
		};
	};
};
