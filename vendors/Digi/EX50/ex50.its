/dts-v1/;

/ {
	description = "Digi/EX50";
	#address-cells = <1>;

	images {
		kernel_1 {
			description = "Linux kernel";
			data = /incbin/("kernel.gz");
			type = "kernel";
			arch = "arm64";
			os = "linux";
			compression = "gzip";
			load = <0x41000000>;
			entry = <0x41000000>;
			hash_1 {
				algo = "sha1";
			};
		};

		fdt_1 {
			description = "Flattened Device Tree blob";
			data = /incbin/("ex50.dtb");
			type = "flat_dt";
			arch = "arm64";
			compression = "none";
			hash_1 {
				algo = "sha1";
			};
		};
	};

	configurations {
		default = "config_cp01-c1";
		config_cp01-c1 {
			description = "Digi/EX50 image";
			kernel = "kernel_1";
			fdt = "fdt_1";
			hash_1 {
				algo = "sha1";
			};
		};
	};
};
