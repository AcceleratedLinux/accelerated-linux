CC='cc -A nansi'    # otherwise matherr() won't load

MATHLIB='      '    # don't need a mathlib

# where to put the binary
BINDIR=/usr/local/bin

# where to put the man pages and man page extension
MANDIR=/usr/local/man/man1
MANEXT=1

