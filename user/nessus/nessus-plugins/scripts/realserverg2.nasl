include("http_func.inc");

port7070 = get_kb_item("Services/realserver");
if(!port7070)port7070 = 7070;

if(get_port_state(port7070))
{
  #
  # A real server is running. Now, let's use the
  # result of the port scan to find the admin port
  #
  if(http_is_dead(port:port7070))exit(0);
  

  i = 1024;
  admin = 0;
 
  while(!admin)
  {
   i = i+1;
   if(get_port_state(i))
   {
    soc = open_sock_tcp(i);
    if(soc)
    {
     req = http_get(item:"/admin/index.html", port:i);
     send(socket:soc, data:req);
     r = http_recv(socket:soc);
     if(r)
     {
      if("Unauthorized" >< r)admin = i;
     }
    http_close_socket(soc);
    }
   }
   if(i > 65535)exit(0);
  }
  
  if(admin)
  {
  head = "GET /admin/index.html HTTP/1.0\r\n" +
	 "Connection: Keep-Alive\r\n" +
	 "User-Agent: Mozilla/4.04  (X11; I; Beavuh OS .9 i486; Nav)\r\n"+
   	 "Host: 111.111.11.1:1111\r\n" + 
	 "Accept: image/gif, image/x-xbitmap, image/jpeg, image/pjpeg, image/png, */*\r\n"+
	"Accept-Language: en\r\n"+
	"Accept-Charset: iso-8859-1,*,utf-8\r\n"+
	"Authorization: Basic ";
mid = "";
   tail = "\r\n\r\n";
   str = string(head, mid, tail);
   soc = open_sock_tcp(admin);
   if(soc)
   {
    send(socket:soc, data:str);
    close(soc);
    sleep(1);
    soc2 = open_sock_tcp(admin);
    send(socket:soc2, data:req);
    rep = http_recv(socket:soc2);
    if(!rep)
    {
    security_hole(admin);
    soc3 = open_sock_tcp(6968);
    if(soc3)
     {
     security_hole(port:6968, data:"We could open a shell on this port, thanks to the RealG2 overflow");
     close(soc3);
     }
    }
   }
  }
}
