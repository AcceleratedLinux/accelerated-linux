
'begin'
	vprint ("printed as real:");
	outreal (1, 1);	outreal (1, -1);
	outstring (1, `\n');
	outreal (1, 100); outreal (1, -100);
	outstring (1, `\n');
	outreal (1, 100000); outreal (1, -100000);
	outstring (1, `\n');
	outreal (1, 1.234); outreal (1, -1.234);
	outstring (1, `\n');
	outreal (1, 1.234'10'10); outreal (1, -1.234'10'10);
	outstring (1, `\n');
	outreal (1, 1.234'10'-10); outreal (1, -1.234'10'-10);
	outstring (1, `\n');
	vprint ("printed as integer:");
	outinteger (1, 1);	outinteger (1, -1);
	outstring (1, `\n');
	outinteger (1, 100); outinteger (1, -100);
	outstring (1, `\n');
	outinteger (1, 100000); outinteger (1, -100000);
	outstring (1, `\n');
	outinteger (1, 1.234); outinteger (1, -1.234);
	outstring (1, `\n');
	outinteger (1, 1.234'10'10); outinteger (1, -1.234'10'10);
	outstring (1, `\n');
	outinteger (1, 1.234'10'-10); outinteger (1, -1.234'10'-10);
	outstring (1, `\n');
	vprint ("done.")
'end'
