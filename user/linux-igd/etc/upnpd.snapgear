#comments stripped - see upnpd.conf in src tree
iptables_location = /bin/iptables
debug_mode = 0
insert_forward_rules = yes
#We use non-standard chains to allow for easier reporting
forward_chain_name = UpnpFwd
prerouting_chain_name = UpnpPF 
upstream_bitrate = 512000
downstream_bitrate = 512000
description_document_name = gatedesc.xml
xml_document_path = /etc/linuxigd
