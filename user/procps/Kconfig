	bool "pkill"

config USER_PROCPS_PMAP
	bool "pmap"

config USER_PROCPS_PS
	bool "ps"

config USER_PROCPS_PWDX
	bool "pwdx"

config USER_PROCPS_SLABTOP
	bool "slabtop"

config USER_PROCPS_SYSCTL
	bool "sysctl"

config USER_PROCPS_TLOAD
	bool "tload"

config USER_PROCPS_TOP
	bool "top"

config USER_PROCPS_UPTIME
	bool "uptime"

config USER_PROCPS_VMSTAT
	bool "vmstat"

config USER_PROCPS_W
	bool "w"

config USER_PROCPS_WATCH
	bool "watch"

