#

include @top_builddir@Rules.make
VPATH = @srcdir@
CFLAGS+=-I@srcdir@/include -I$(KERNEL_DIR)/include

libipulog.a: libipulog.o
	$(LD) -i $< -o $@

distrib:

ulog_test: ulog_test.c libipulog.a
	$(CC) $(CFLAGS) ulog_test.c libipulog.a -o ulog_test

libipulog.o: libipulog.c
	$(CC) $(CFLAGS) -c $< -o $@

clean:
	$(RM) ulog_test libipulog.o libipulog.a

distclean: clean
	$(RM) Makefile

install: libipulog.a
